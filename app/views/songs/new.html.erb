<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.text_field :artist_name, list: "artist_autocomplete" %>
  <datalist id="artist_autocomplete">
    <% Artist.all.each do |artist| %>
      <option value="<%= artist.name %>">
    <% end %>
  </datalist>
  <br>
  Genre:
 <%= f.collection_select :genre_id, Genre.all, :id, :name, prompt: true %> <br>
 <input name="song[note_contents][]" id="song_notes_1" value="Note 1"> <br>
 <input name="song[note_contents][]" id="song_notes_2" value="Note  2"> <br>
 <input name="song[note_contents][]" id="song_notes_3" value="Note 3"> <br>
 <input type="submit" value="Submit">

Song:
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br />

  <%= f.submit %>
<% end %>


